{
  "name": "cecil/theme-comingsoon",
  "type": "cecil-theme",
  "description": "Cecil theme: Coming Soon",
  "keywords": ["Cecil", "theme", "coming soon"],
  "license": "MIT",
  "require": {
    "cecil/theme-installer": "^1.4"
  },
  "extra": {
    "name": "comingsoon",
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "cecil/theme-installer": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@post-cmd"
    ],
    "post-update-cmd": [
      "@post-cmd"
    ],
    "post-cmd": [
      "npm install --prefix ./static bootstrap@4.6.0"
    ]
  }
}
